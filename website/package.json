{
  "name": "waypoint-docs",
  "description": "Documentation website for Waypoint",
  "version": "1.0.0",
  "author": "HashiCorp",
  "dependencies": {
    "@hashicorp/mktg-global-styles": "^2.0.0",
    "@hashicorp/nextjs-scripts": "13.0.0-canary.1",
    "@hashicorp/react-alert-banner": "^5.0.0",
    "@hashicorp/react-button": "^4.0.0",
    "@hashicorp/react-content": "6.0.0",
    "@hashicorp/react-docs-page": "^8.0.0-canary.2",
    "@hashicorp/react-hashi-stack-menu": "^1.0.10",
    "@hashicorp/react-head": "1.1.6",
    "@hashicorp/react-image": "3.0.3",
    "@hashicorp/react-product-downloader": "^7.0.0",
    "@hashicorp/react-section-header": "^3.0.1",
    "@hashicorp/react-subnav": "7.0.0",
    "@hashicorp/react-tabs": "^2.0.0",
    "@hashicorp/react-vertical-text-block-list": "^3.0.1",
    "fs-exists-sync": "^0.1.0",
    "gray-matter": "4.0.2",
    "js-yaml": "^3.14.0",
    "line-reader": "^0.4.0",
    "next": "9.5.3",
    "next-mdx-remote": "^1.0.0",
    "nuka-carousel": "^4.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intersection-observer": "^8.30.3",
    "readdirp": "3.5.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "next-remote-watch": "^0.2.0",
    "post-npm-install": "^2.0.0",
    "prettier": "2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "next-hashicorp precommit"
    }
  },
  "main": "index.js",
  "scripts": {
    "prebuild": "if [[ $VERCEL_GIT_COMMIT_REF = 'main' ]]; then echo 'User-agent: * Disallow: /' >> './public/robots.txt'; fi",
    "build": "node --max-old-space-size=2048 ./node_modules/.bin/next build",
    "export": "node --max-old-space-size=2048 ./node_modules/.bin/next export",
    "format": "next-hashicorp format",
    "generate:component": "next-hashicorp generate component",
    "generate:readme": "next-hashicorp markdown-blocks README.md",
    "lint": "next-hashicorp lint",
    "start": "next-remote-watch ./content/**/*.mdx",
    "static": "npm run build && npm run export"
  }
}
